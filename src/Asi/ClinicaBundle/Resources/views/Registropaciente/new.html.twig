<!DOCTYPE html>
<html lang="es">
    <head>

        {% image '@AsiClinicaBundle/Resources/public/img/favicon.ico' %}
        <link rel="shortcut icon" href="{{ asset_url }}" />
        {% endimage %}

        <title>.: Cl&iacute;nica Central :.</title>

        {% block stylesheets %}
            {% stylesheets 

                '@AsiClinicaBundle/Resources/public/css/familyDroid.css'
                'bundles/asiclinica/css/style.css'
                
                filter='cssrewrite'
            %}
            
            <link rel="stylesheet" href="{{ asset_url }}" />
            {% endstylesheets %}
        {% endblock %}

        {% block javascripts %}
            {% javascripts 
                '@AsiClinicaBundle/Resources/public/js/jquery.min.js'
                '@AsiClinicaBundle/Resources/public/js/script.js' 
            %}
            
            <script type="text/javascript" src="{{ asset_url }}"></script>
            {% endjavascripts %}
        {% endblock %}

    </head>
<body>
<!-- TOP BAR -->
    <div id="top-bar">
        
        <div class="page-full-width">
        </div> <!-- end full-width -->  
        </div> <!-- end full-width -->  
    
    </div> <!-- end top-bar -->
    
    
    
    <!-- HEADER -->
    <div id="header">
        
        <div class="page-full-width cf">
    
            <div id="login-intro" class="fl">
            
                <h1>Ingreso de datos de Pendiente</h1>
            
            </div> <!-- login-intro -->
            
            <!-- Change this image to your own company's logo -->
            <!-- The logo will automatically be resized to 39px height. -->
            <a href="#" id="company-branding" class="fr">
                {% image '@AsiClinicaBundle/Resources/public/img/logo.png' %}
                    <img src="{{ asset_url }}" />
                {% endimage %}
            </a>
            
        </div> <!-- end full-width -->  

    </div> <!-- end header -->

<div id="content" >
{% block content %}
                    
    <div style="width:300px; margin: 0 auto;">{{ form(form)}}</div>
{% endblock %}

</div>
    {% include 'AsiClinicaBundle::_footer.html.twig' %}
</body>
</html> 

